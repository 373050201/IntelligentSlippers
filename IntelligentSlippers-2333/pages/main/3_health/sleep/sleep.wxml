<text class='title'>睡眠💤</text>

 <view class='history-table-wrap'>
    <view class="table">
      <view class="tr">
          <view class="th th1">日期</view>
          <view class="th th2">上床</view>
          <view class="th th3">起床</view>
          <view class="th th4">参考睡眠时间</view>
      </view>
      <view class="tr" wx:for="{{arrayList}}">
          <view class="td td1">{{item.date}}</view>
          <view class="td td2">{{item.bedTimeHour}}{{item.colon1}}{{item.bedTimeMinute}}</view>
          <view class="td td3">{{item.wakeTimeHour}}{{item.colon2}}{{item.wakeTimeMinute}}</view>
          <view class="td td4"><view class='{{item.colorState}}'>{{item.difHour}}{{item.h}}{{item.difMinute}}{{item.min}}</view></view>
      </view>
    </view>
 </view>

 <button class='buttonReturn' plain='true' bindtap='jumpHealth'><image src="/pages/images/more/return.png"></image></button>